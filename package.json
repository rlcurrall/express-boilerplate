{
  "name": "express-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate code for an Express Typescript Application",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/index.js",
