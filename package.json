    "@types/helmet": "0.0.44",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-plugin-node": "^10.0.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
